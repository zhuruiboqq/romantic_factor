<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.romantic.factor.dao.Message2Dao">
	
	<insert id="insert" parameterType="message2">
		insert into
		message2(ctime,name,qqNum,telPhone,email,address,suggestion)
		values(#{ctime},#{name},#{qqNum},#{telPhone},#{email},#{address},#{suggestion});
	</insert>



	

</mapper>